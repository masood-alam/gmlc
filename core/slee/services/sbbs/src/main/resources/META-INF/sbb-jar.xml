<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">
<sbb-jar>
	<sbb id="gmlc_gw_parent">
		<description>Represents the entire location retrieval</description>
		<sbb-name>MobileCoreNetworkInterfaceSbb</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>1.0</sbb-version>
		<sbb-alias>MobileCoreNetworkInterfaceSbb</sbb-alias>

		<library-ref>
			<library-name>gmlc-library</library-name>
			<library-vendor>org.mobicents</library-vendor>
			<library-version>2.0</library-version>
		</library-ref>

		<sbb-ref>
			<sbb-name>CDRSbbPlain</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>CDRSbb_plain</sbb-alias>
		</sbb-ref>

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.gmlc.slee.map.MobileCoreNetworkInterfaceSbb
				</sbb-abstract-class-name>
				<cmp-field>
					<cmp-field-name>eventContext</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<cmp-field-name>eventContextCMP</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<cmp-field-name>httpRequest</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<cmp-field-name>timerID</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<cmp-field-name>gMLCCDRState</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<cmp-field-name>anyTimeInterrogationRequestInvokeId</cmp-field-name>
				</cmp-field>

				<get-child-relation-method>
					<sbb-alias-ref>CDRSbb_plain</sbb-alias-ref>
					<get-child-relation-method-name>getCDRPlainInterfaceChildRelation</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
			</sbb-abstract-class>
		</sbb-classes>
		<!-- SS7 -->
		<event event-direction="Receive" initial-event="False">
			<event-name>AnyTimeInterrogationRequest</event-name>
			<event-type-ref>
				<event-type-name>ss7.map.service.mobility.subscriberinfo.ANY_TIME_INTERROGATION_REQUEST
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>
		<event event-direction="Receive" initial-event="False">
			<event-name>AnyTimeInterrogationResponse</event-name>
			<event-type-ref>
				<event-type-name>ss7.map.service.mobility.subscriberinfo.ANY_TIME_INTERROGATION_RESPONSE
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>
		<event event-direction="Receive" initial-event="False">
			<event-name>SendRoutingInfoForLCSRequest</event-name>
			<event-type-ref>
				<event-type-name>ss7.map.service.lsm.SEND_ROUTING_INFO_FOR_LCS_REQUEST
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>
		<event event-direction="Receive" initial-event="False">
			<event-name>SendRoutingInfoForLCSResponse</event-name>
			<event-type-ref>
				<event-type-name>ss7.map.service.lsm.SEND_ROUTING_INFO_FOR_LCS_RESPONSE
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>
		<event event-direction="Receive" initial-event="False">
			<event-name>ProvideSubscriberLocationRequest</event-name>
			<event-type-ref>
				<event-type-name>ss7.map.service.lsm.PROVIDE_SUBSCRIBER_LOCATION_REQUEST
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>
		<event event-direction="Receive" initial-event="False">
			<event-name>ProvideSubscriberLocationResponse</event-name>
			<event-type-ref>
				<event-type-name>ss7.map.service.lsm.PROVIDE_SUBSCRIBER_LOCATION_RESPONSE
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
